<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bcp.androidchallenge">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.CustomApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo_app_bcp_banca_movil"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_app_bcp_banca_movil"
        android:supportsRtl="true"
        android:theme="@style/Theme.GozzaloApp">
        <activity android:name=".application.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>